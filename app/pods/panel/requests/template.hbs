{{page-title "Requests"}}
Requests panel
<div class="container">
  <div class="row row-cols-3">
    {{#each this.model as |request index|}}
      {{! template-lint-disable no-inline-styles }}
      <div
        data-test-card={{index}}
        class="card col justyfy-content-between m-2"
        style="width: 18rem;"
      >
        <div class="card-header">
          Pro≈õba wizyty #{{request.id}}
        </div>
        <ul class="list-group list-group-flush">
          <li
            data-test-card-name={{index}}
            class="list-group-item"
          >{{request.name}}</li>
          <li
            data-test-card-surname={{index}}
            class="list-group-item"
          >{{request.surname}}</li>
        </ul>
      </div>
    {{/each}}
  </div>
</div>
{{outlet}}